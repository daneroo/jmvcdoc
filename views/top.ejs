<div class='top'>
    <h1><%=name.replace(/~/g,".") %><span class='<%= shortName%> type'><%= shortName %></span><span class="favorite favorite<%= isFavorite? 'on' : 'off'%>">&nbsp;&nbsp;&nbsp;</span></h1>
    <% if(this.inherits){ %>
    <div class='inherits'>
        inherits: <%= linkOpen(this.inherits) %>
    </div>
    <%} %>
    <% if(this.tags){ %>
    <div class='tags'>
        tags: <%= linkTags(this.tags) %>
    </div>
    <%} %>
    <% if(this.plugin){ %>
    <div class='plugin'>
        Plugin: <%= this.plugin %>
    </div>
    <% } %>
    <% if(this.iframe){ %>
    <div class='iframe_wrapper'>
        <button class='iframe_menu_button class="ui-button ui-widget ui-state-default ui-corner-top ui-button-text-only'>Scripts</button>
        <iframe src="<%= iframe.src %>" height="<%= iframe.height %>" frameborder=0></iframe>
    </div>
    <% } %>
</div>
