<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
            "http://www.w3.org/TR/html4/strict.dtd">
<html lang="en">
	<head>
		<title>JavaScriptMVC - <%= title %></title>
        <link rel="stylesheet" type='text/css' href='<%= jmvcRoot %>documentjs/jmvcdoc/style.css' />
        <link rel="shortcut icon" href="<%= jmvcRoot %>documentjs/jmvcdoc/images/favicon.ico" />
	</head>
	<body>
	    
        <div id='documentation'>
	        <div id='top'>
                <div id='defaults'>
                	<input id='search' type='input' class='notFocused' /> 
					<ul id="menu" class="ui-menu">
						<li class="ui-menu-item">
                    		<a href="#&amp;search=&#42;&amp;who=index">Home</a>
						</li>
						<li class="ui-menu-item">						
	                    	<a href="#&amp;search=core">Core</a>
						</li>
						<li class="ui-menu-item">						
	                    	<a href="#favorites">Favorites</a>
						</li>
						<li class="ui-menu-item">						
							<a href="http://javascriptmvc.com/#&who=follow" class="red" title="Follow">Follow</a>
						</li>
						<li class="ui-menu-item">						
							<a href="http://github.com/jupiterjs/framework" class="red" title="Code Repositories">Code Repositories</a>
						</li>
					</ul>
                    <div id='logo'>
                    	JavaScript
                    	<img src='<%= jmvcRoot %>documentjs/jmvcdoc/images/logo.png' style="position: relative; top: 5px;left: 2px"/>
                    </div>
                    
                </div>
	        </div>
            
            <div id='bottom'>
                
            
                <div id='left'>
                    <%= sidebar %>
                </div>
                <div id='doc_container'>
                    <div id='doc'>
                        <%= body %>
                    </div>
                </div>
            </div>
        </div>
        <div id='low'>
            <a href="http://jupiterit.com">&copy; Jupiter IT - JavaScriptMVC Training and Support</a>
        </div>
		<script type='text/javascript'>
            DOCS_LOCATION = "<%= jmvcRoot %><%= args.path %>/"
            <% if(args.commentsLoc){ %>
            COMMENTS_LOCATION = "<%= args.commentsLoc %>";
            <% } %>
        </script>
        <script type='text/javascript' 
                src='<%= jmvcRoot %>steal/steal.production.js?documentjs/jmvcdoc'>   
        </script>
        <% if(args.analyticsAct){ %>
        	
		<script type="text/javascript">
			var _gaq = _gaq || [];
			_gaq.push(['_setAccount', "<%= args.analyticsAct %>"]);
			_gaq.push(['_trackPageview']);
			_gaq.push(['_setDomainName', '.<%= args.analyticsDomain %>']);
			_gaq.push(['_setAllowLinker', true]);
			_gaq.push(['_setAllowHash', false]);
			
			(function() {
			var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
			ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
			var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
			})();
		</script> 
		
        <% } %>
	</body>
</html>